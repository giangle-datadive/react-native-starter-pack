{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "react-native"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "extends": "airbnb",
  "rules": {
    "arrow-body-style": "warn",
    "semi": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/forbid-prop-types": 0,
    "react/no-array-index-key": 0,
    "import/no-cycle": 0,
    "import/prefer-default-export": 0,
    "react/jsx-one-expression-per-line": 0
  },
  "globals": {
    "alert": true
  }
}